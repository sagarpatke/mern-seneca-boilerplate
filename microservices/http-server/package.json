    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.12.7",
    "jwt-microservice": "file:../microservice-jwt",
    "mocha": "^2.5.3",
    "react-hot-loader": "^1.3.0",
    "should": "^9.0.2",
