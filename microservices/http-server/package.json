    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.0",
    "react": "^15.1.0",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.1.0",
